<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BethanysPieShopMobile.NavigationPageView">
    <ContentPage.Content>
        <StackLayout>
            <Button x:Name="btnBasket" Text="Basket" Clicked="btnBasket_Clicked"/>
            <Button x:Name="btnContactUs" Text="Contact us" Clicked="btnContactUs_Clicked" />
            <Button x:Name="btnHome" Text="Home" Clicked="btnHome_Clicked"/>
            <Button x:Name="btnPiesOverview" Text="Pies" Clicked="btnPiesOverview_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>